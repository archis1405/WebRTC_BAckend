{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rules": {
        "no-console": false,
        "semicolon": [true, "always"],
        "eofline": true,
        "quotemark": [true, "double"],
        "indent": [true, "spaces", 4],
        "trailing-comma": [false]
    },
    "jsRules": {}
}